  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
    commit: 6d2362550056094630ed1710553eb8e69b505770
