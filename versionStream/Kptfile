metadata:
  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
  updateStrategy: force-delete-replace
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
    commit: d7584f83b82c40ffcb72891ac542d10fe27d983c
