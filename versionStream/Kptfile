  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
    commit: f9c3577506dd309cc1a9df678821b5a45c23c57a
