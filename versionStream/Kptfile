  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
    commit: 3f1356347a908fe2e56fa24239026f8307eaea1c
