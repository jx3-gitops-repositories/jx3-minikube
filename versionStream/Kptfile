  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
    commit: 1296c83a3cdb38d4c1cb475053f3586c73b6c8c5
