apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
    commit: fb3759c0b51d38fe60fe9685b35da25e2436c71c
