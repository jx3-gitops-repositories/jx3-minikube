  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
  updateStrategy: force-delete-replace
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
    commit: 984c71ddb178e885fbed56c96d9dc803b91a5fc7
