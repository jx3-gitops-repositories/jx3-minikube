metadata:
  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jxr-versions
    directory: /
    ref: master
    commit: 8f45286dcf430eaad71e8b0315cb2ffba63994bd
