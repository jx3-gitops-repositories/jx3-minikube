  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
    commit: efd50a0e98cd17042264e12f1759dce401fc9128
